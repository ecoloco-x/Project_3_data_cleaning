   "source": [
    "# start cleaning\n",
    "pollution['Estimated Litres'].fillna('Unknown', inplace = True)\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].str.lower()\n",
    "\n",
    "to_remove = [' ', ',', '>', '<','kg','litres', 'litre'] # first clean up, removing units and symbols\n",
    "                                                        # (assuming 1kg is 1 litre for simplicity)\n",
    "\n",
    "for row,i in pollution['Estimated Litres'].iteritems():\n",
    "    print(row,i)\n",
    "    for r in to_remove:\n",
    "        i = i.replace(r, '') # remove characters in the list\n",
    "    if i.endswith(\"l\"):\n",
    "        i = i.replace(\"l\", \"\")\n",
    "    if i == 'unknown':\n",
    "        continue\n",
    "    try:\n",
    "        i = float(i) # after first clean up, check if it's a real number\n",
    "        pollution.loc[row, 'Estimated Litres']=i\n",
    "    except:\n",
    "        print('to clean: ', i) # if not, start second round of cleaning\n",
    "        r = re.findall(r'\\d+-\\d+', i) # look up for range like values\n",
    "        s = re.findall(r'\\d+m?x\\d+m',i) # look up for surface like values\n",
    "        \n",
    "        \n",
    "        if r: # when its a range, take out the string in returned list and calculate the mean of range\n",
    "            r = r[0] \n",
    "            num = r.split('-')\n",
    "            avg = (int(num[0])+int(num[1]))/2\n",
    "            i = avg\n",
    "            print('cleaned: ',i)\n",
    "            pollution.loc[row, 'Estimated Litres']=i\n",
    "            \n",
    "            \n",
    "        elif s:  # when its a surface, take out the string in returned list and calculate the volumn\n",
    "                 # formula used : 1000m x 1000m x 0.000001m  = 1 m3 = 1000 litres\n",
    "            s = s[0]\n",
    "            s = s.replace(\"m\",\"\")\n",
    "            num = s.split('x')\n",
    "            sur = int(num[0])*int(num[1])\n",
    "            vol = sur * 0.001\n",
    "            i = vol\n",
    "            print('cleaned: ',i)\n",
    "            pollution.loc[row, 'Estimated Litres']=i\n",
    "\n",
    "\n",
    "        else: # for the rest just set as unknow\n",
    "            print('cant be cleaned')\n",
    "            i = 'unknown'\n",
    "            pollution.loc[row, 'Estimated Litres']=i\n",
    "        \n",
    "\n",
    "# pd.set_option('display.max_rows',2222)\n",
    "# pollution[pollution['Estimated Litres']!='unknown']\n"
   ]
