    "# start cleaning\n",
    "pollution['Estimated Litres'].fillna('Unknown', inplace = True)\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].str.lower()\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].replace(',','')\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].replace(' ','')\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].replace('<','')\n",
    "pollution['Estimated Litres'] = pollution['Estimated Litres'].replace('>','')\n",
    "\n",
    "\n",
    "bad_lst = []\n",
    "good_lst = []\n",
    "for i in pollution['Estimated Litres']:\n",
    "    if i == 'unknown':\n",
    "        bad_lst.append(i)\n",
    "        continue\n",
    "    elif i.isnumeric():\n",
    "        good_lst.append(i)\n",
    "        continue\n",
    "    elif re.findall(r'\\d\\.\\d',i):\n",
    "        try:\n",
    "            i = float(i)\n",
    "            good_lst.append(i)\n",
    "        except:\n",
    "            i = 'unknown'\n",
    "            print('###############################', i)\n",
    "            bad_lst.append(i)\n",
    "    elif i.endswith('l'):\n",
    "        i = i.replace('l', '')\n",
    "        good_lst.append(i)\n",
    "    else:\n",
    "        print(i)\n",
    "        bad_lst.append(i)\n",
    "        \n",
    "print('bad',len(bad_lst))\n",
    "print('ok', len(good_lst))"
   ]
